{
  "name": "astro-partykit-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"partykit dev\" \"astro dev\"",
    "start": "astro dev",
    "build": "astro check && astro build && partykit deploy",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^5.5.4",
    "@astrojs/react": "^4.0.0",
    "@astrojs/tailwind": "^5.1.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "astro": "^4.16.17",
    "partysocket": "1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.17.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "partykit": "0.0.111",
    "prettier": "3.4.1",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro,mjs,mts,json,cjs}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,astro,mjs,mts,json,cjs}": "prettier --write",
    "*.{ts,tsx}": "vitest related --run"
  }
}
