{
  "name": "astro-partykit-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"partykit dev\" \"astro dev\"",
    "start": "astro dev",
    "build": "astro check && astro build && partykit deploy",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^6.1.0",
    "@astrojs/react": "^4.1.3",
    "@astrojs/tailwind": "^5.1.4",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "astro": "^5.1.5",
    "partysocket": "1.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "partykit": "0.0.111",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro,mjs,mts,json,cjs}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,astro,mjs,mts,json,cjs}": "prettier --write",
    "*.{ts,tsx}": "vitest related --run"
  }
}
