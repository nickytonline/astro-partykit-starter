{
  "name": "astro-partykit-starter",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"partykit dev\" \"astro dev\"",
    "start": "astro dev",
    "build": "astro check && astro build && partykit deploy",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/netlify": "^5.5.2",
    "@astrojs/react": "^3.6.2",
    "@astrojs/tailwind": "^5.1.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.15.4",
    "partysocket": "1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.4.0",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "partykit": "0.0.80",
    "prettier": "3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro,mjs,mts,json,cjs}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,astro,mjs,mts,json,cjs}": "prettier --write",
    "*.{ts,tsx}": "vitest related --run"
  }
}
